{
  "name": "coursewebrest",
  "version": "0.1.0",
  "description": "Restful API Server for the Courseweb",
  "main": "server.js",
  "scripts": {
    "install": "npm install && cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sachith Rukshan",
  "license": "GNU GPL",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2"
  }
}